<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".view.activity.UserQrPage">

    <fragment
            android:id="@+id/userqr_actbar"
            android:layout_width="match_parent"
            android:name="com.example.spb.view.Component.FragmentSpbAvtivityBar"
            android:layout_height="@dimen/spb_activity_bar_44"/>

    <ImageView
            android:id="@+id/my_qrCode"
            android:scaleType="center"
            android:layout_marginLeft="25dp"
            android:src="@color/qianbai"
            android:layout_width="150dp"
            android:layout_height="150dp"/>

</RelativeLayout>